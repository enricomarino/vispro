<?xml version="1.0" encoding="UTF-8"?>

<!-- Start XML configuration -->
<vispro xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:noNamespaceSchemaLocation="descriptor.xsd">
    <types>
        <type name="zoom" />
        <type name="map" />
        <type name="search" />
        <type name="info" />
    </types>
    <!-- Components list -->
    <widgets>

        <!-- Widget 1 -->
        <widget type="zoom" name="zoom_h_s">
            <label>Horizontal Scroll Zoom</label>
            <description>Zoom orizzontale con scrollbar</description>
            <images>
                <icon url="images/zoom_h_s.jpg" />
                <image url="images/zoom_h_s.jpg" />
            </images>
            <dimensions>
                <width value="168" min="10" max="300" resizable="true"/>
                <height value="26" resizable="false"/>
            </dimensions>
            <dependencies>
                <dependency name="request_map" required="true" type="map">
                    <description>La mappa su cui agisce lo zoom</description>
                </dependency>
            </dependencies>
            <properties>
                <bool name="tooltip" label="Tooltip property" default="true"/>
            </properties>
            <code>
                <insert match="html">
<![CDATA[
<div id="<%= id %>" style="position:absolute; top:<%= top %>px; left:<%= left %>px; z-index: <%= zIndex %>" ></div>
]]>
                </insert>
                <insert match="js">
<![CDATA[
var <%= id %> = geopoi.sdk.zoom("<%= id %>", { 
        map: <%= request_map %>, 
        tooltip: <%= tooltip %>,
        vertical: false,
        slider: true,
        size: <%= width %>
    });
]]>
                </insert>
            </code>
        </widget>


        <!-- Widget 2 -->
        <widget type="zoom" name="zoom_v_s">
            <label>Vertical Scroll Zoom</label>
            <description>Zoom verticale con scrollbar</description>
            <images>
                <icon url="images/zoom_v_s.jpg" />
                <image url="images/zoom_v_s.jpg" />
            </images>
            <dimensions>
                <width value="26" resizable="false"/>
                <height value="168" min="10" max="300" resizable="true"/>
            </dimensions>
            <dependencies>
                <dependency name="request_map" required="true" type="map" label="Mappa">
                    <description>La mappa su cui agisce lo zoom</description>
                </dependency>
            </dependencies>
            <properties>
                <bool name="tooltip" label="Tooltip property" default="false"/>
            </properties>
            <code>
                <insert match="html">
<![CDATA[
<div id="<%= id %>" style="position:absolute; top:<%= top %>px; left:<%= left %>px; z-index: <%= zIndex %>"></div>
]]>
                </insert>
                <insert match="js">
<![CDATA[
var <%= id %> = geopoi.sdk.zoom("<%= id %>", { 
        map: <%= request_map %>, 
        tooltip: <%= tooltip %>,
        vertical: true,
        slider: true,
        size: <%= width %>
    });
]]>
                </insert>
            </code>
        </widget>


        <!-- Widget 3 -->
